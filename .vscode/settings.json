{"launch": {
        "version": "0.2.0",
        "configurations": [{
            "name": "Local Debugging",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/myhttp",
            "args": ["--port", "8080"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/home/florian/Projects/PacketSniffer/sudo-gdb.sh",
            "preLaunchTask": "Build myhttp",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }],
        "compounds": []
    }}